{
  "id": "sandbox",
  "title": "Sandbox System Builder",
  "description": "Custom system builder to build character sheets and rulesets for any game.",
  "version": "0.14.0(Beta)",
  "compatibility": {
    "minimum": "10",
    "verified": "10.291",
    "maximum": "10"
  },
  "templateVersion": 1,
  "authors": [{
      "name": "Seregras"
    }, {
      "name": "Alondaar"
    }, {
      "name": "Ramses800"
    }],
  "esmodules": [
    "module/sandbox.js",
    "module/sb-tools-form.js",
    "module/sb-api.js",
    "module/dropdownmenu.js",
    "module/sb-settings-registration.js",
    "module/system-settings-form.js",
    "module/sb-bug-report.js",
    "module/sb-expression-editor-form.js",
    "module/sb-expression-editor-code-mirror.js",
    "module/sb-itemsheet-constants.js",
    "module/sb-itemsheet-helpers.js",
    "module/sb-key-validate.js",
    "module/sb-table-filter-editor-form.js"
  ],
  "styles": [
    "styles/sandbox.css",
    "styles/dropdownmenu.css",
    "styles/sb-codemirror.css",
    "styles/system-settings.css"
  ],
  "relationships": {    
    "requires": [{
      "id": "_CodeMirror",
      "type": "module",
      "manifest": "https://raw.githubusercontent.com/League-of-Foundry-Developers/codemirror-lib/master/module.json",
      "compatibility": {
        "verified": "5.58.3-fvtt5"
      }
    }]
  },
  "packs": [],
  "socket": true,  
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],
  "gridDistance": 5,
  "gridUnits": "ft",
  "primaryTokenAttribute": "health",
  "secondaryTokenAttribute": "power",
  "url": "https://gitlab.com/rolnl/sandbox-system-builder/",
  "manifest": "https://gitlab.com/rolnl/sandbox-system-builder/raw/dev_0.14.0/system.json",
  "download": "https://gitlab.com/rolnl/sandbox-system-builder/-/archive/dev_0.14.0/sandbox-master.zip",
  "changelog": "https://gitlab.com/rolnl/sandbox-system-builder/raw/dev_0.14.0/changelog.md"
}
