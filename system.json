{
  "id": "sandbox",
  "title": "Sandbox System Builder",
  "description": "Custom system builder to build character sheets and rulesets for any game.",
  "version": "0.14.4.5",
  "compatibility": {
    "minimum": "10",
    "verified": "12"
  },
  "templateVersion": 1,
  "authors": [{
      "name": "Seregras"
    }, {
      "name": "Alondaar"
    }, {
      "name": "Ramses800"
    }],
  "esmodules": [
    "module/sandbox.js"
  ],
  "styles": [
    "styles/sandbox.css",
    "styles/dropdownmenu.css",
    "styles/sb-codemirror.css",
    "styles/system-settings.css",
    "styles/sandbox-icons-font.css"
  ],
  "relationships": {    
    "requires": [{
      "id": "_CodeMirror",
      "type": "module",
      "manifest": "https://raw.githubusercontent.com/League-of-Foundry-Developers/codemirror-lib/master/module.json",
      "compatibility": {
        "verified": "5.58.3-fvtt5"
      }
    }]
  },
  "packs": [],
  "socket": true,  
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],
  "gridDistance": 5,
  "gridUnits": "ft",
  "primaryTokenAttribute": "health",
  "secondaryTokenAttribute": "power",
  "url":       "https://raw.githubusercontent.com/BBurns42/sandbox-system-builder/",
  "manifest":  "https://raw.githubusercontent.com/BBurns42/sandbox-system-builder/refs/heads/master/system.json",
  "download":  "https://github.com/BBurns42/sandbox-system-builder/archive/refs/tags/v0.14.4.5.zip",
  "changelog": "https://raw.githubusercontent.com/BBurns42/sandbox-system-builder/refs/heads/master/changelog.md"
}
